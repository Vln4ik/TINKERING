#!/usr/bin/env sh
#
# Minimal gradlew stub. If Android Studio complains about missing wrapper JAR,
# generate wrapper files from Android Studio or run:
#   gradle wrapper --gradle-version 8.7
#
echo "Gradle wrapper JAR is not included in this repo snapshot."
echo "Open the project in Android Studio and let it generate/download wrapper files,"
echo "or run: gradle wrapper --gradle-version 8.7"
exit 1


